<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <title>@ViewData["Title"] - Church of the Emperor-as-Messiah</title>
    <script src="/lib/jquery/dist/jquery-3.3.1.js"></script>
    <link rel="apple-touch-icon" sizes="180x180" href="/Assets/Icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/Assets/Icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/Assets/Icons/favicon-16x16.png">
    <link rel="manifest" href="/Assets/Icons/site.webmanifest">
    <link rel="shortcut icon" href="/Assets/Icons/favicon.ico">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-config" content="/Assets/Icons/browserconfig.xml">
    <meta name="theme-color" content="#ffffff"> @RenderSection("Styles", false)
</head>

<body>

    <div class="center">
        <div class="container-fluid">
            <div class="row-fluid" style="width:90%; margin-left: auto; margin-right: 5%;">
                <div class="col col-centered">
                    <nav class="navbar navbar-expand-lg navbar-light bg-light">
                        <div class="img-fluid navbar-brand">
                            <img src="~/Assets/Twitter_Header.png" style="width:100%;" />
                        </div>
                        <button class="navbar-toggler float-right" type="button" data-toggle="collapse" data-target="#navbarColor03" aria-controls="navbarColor03" aria-expanded="false" aria-label="Toggle navigation" style="display:none;">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse show navbar-collapse float-right" id="navbarColor03" style="font-size: 14pt;   ">
                            <ul class="navbar-nav mr-auto">
                                <li class="nav-item">
                                    <a class="nav-link" href="~/">Home</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/Decrees">Decrees</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/About">About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/Join">Join the Church</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/10Blessings">The Ten Virtues</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/Reading">Readings</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/Social">Important Links</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" href="~/FAQ">HC FAQ</a>
                                </li>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </div>



    <div class="center">
        <div class="container-fluid body-content">
            <div class="row-fluid" style="width:90%; margin-left: auto; margin-right: 5%;">
                <div class="col col-centered">
                    @RenderBody()
                </div>
            </div>
            <div style="height:35px;"></div>
            <div class="row-fluid" style="width:90%; margin-left: auto; margin-right: 5%; font-size: larger">
                <div class="col">
                    <footer>
                        <div class="container-fluid">
                            <div class="row-fluid w-100">
                                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 col-xl-3">
                                    <a href="/Support">Support the Developers</a>
                                </div>
                                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 col-xl-3">
                                    <a href="https://github.com/kbmacneal/church_of_the_emperor_as_messiah" target="_blank">This site is licensed under the MIT licence</a>
                                </div>
                                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 col-xl-3">
                                    <a href="~/FAQ">HC FAQ</a>
                                </div>
                                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 col-xl-3">

                                </div>
                            </div>
                        </div>
                        <p>

                        </p>

                    </footer>
                </div>
            </div>
        </div>
    </div>



    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery-3.3.1.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js" asp-fallback-src="~/lib/jquery/dist/jquery.min.js" asp-fallback-test="window.jQuery" crossorigin="anonymous" integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js" asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js" asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal" crossorigin="anonymous" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)

</body>

</html>